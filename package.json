{
  "name": "wxminiapp2vue",
  "version": "1.0.0",
  "description": "Parser wxml to angular template",
  "main": "dist/index.js",
  "scripts": {
    "build:ts": "rimraf ./dist && tsc",
    "build:ui": "cross-env NODE_ENV=production webpack --config kbone-ui/build/webpack.common.js --progress --hide-modules",
    "build:weui": "gulp compile --gulpfile kbone-ui/weui/gulpfile.js",
    "build:weui-v1": "gulp compile --gulpfile kbone-ui/weui-v1/gulpfile.js",
    "build": "rimraf kbone-ui-lib && npm run build:ui && npm run build:weui-v1 && npm run build:ts",
    "watch": "nodemon --watch ./src -e ts,js,json,html --exec './local_start.sh'",
    "server": "node ./proxy/index2.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/livoras/wxml2angular.git"
  },
  "keywords": [
    "wxml",
    "angular"
  ],
  "author": "Livoras",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/livoras/wxml2angular/issues"
  },
  "homepage": "https://github.com/livoras/wxml2angular#readme",
  "dependencies": {
    "@types/node": "^13.11.1",
    "@vivaxy/wxml": "^1.0.0",
    "axios": "^0.19.2",
    "commander": "^5.0.0",
    "html-webpack-plugin": "^4.2.0",
    "parse5": "^5.1.1",
    "ts-node": "^8.8.1",
    "vue": "^2.6.11",
    "vue2-touch-events": "^2.2.1",
    "weixin-js-sdk": "^1.3.3",
    "weixin-jsapi": "^1.1.0",
    "weixinjssdk": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "^13.13.2",
    "@types/parse5": "^5.0.2",
    "acorn": "^8.10.0",
    "acorn-walk": "^8.2.0",
    "babel-eslint": "^9.0.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^0.28.0",
    "escodegen": "^2.1.0",
    "eslint": "^4.15.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^4.0.0",
    "estraverse": "^5.3.0",
    "globby": "^10.0.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-less": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy": "^1.18.1",
    "jest": "^24.9.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^3.0.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "rimraf": "^2.7.1",
    "terser-webpack-plugin": "^2.2.1",
    "typescript": "^5.1.6",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  }
}
