<xr-demo-viewer
  title="双面半透明模型"
  intro="加载模型后，手动设置双面渲染状态"
>
  <xr-template-blendDouble
    disable-scroll
    id="main-frame"
    width="{{renderWidth}}"
    height="{{renderHeight}}"
    style="width:{{width}}px;height:{{height}}px;top:{{top}}px;left:{{left}}px;display:block;"
  />
</xr-demo-viewer>
