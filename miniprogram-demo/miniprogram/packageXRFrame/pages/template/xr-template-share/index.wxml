<xr-demo-viewer
  title="分享截屏案例"
  intro="通过分享系统截屏分享，点击画布右下角1/3区域触发。"
>
  <xr-template-share
    disable-scroll
    id="main-frame"
    width="{{renderWidth}}"
    height="{{renderHeight}}"
    style="width:{{width}}px;height:{{height}}px;top:{{top}}px;left:{{left}}px;display:block;"
  />
</xr-demo-viewer>
