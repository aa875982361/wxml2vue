<xr-demo-viewer
  title="AR面向屏幕的平面"
  intro="通过脚本控制的，四个方向各有一个面向屏幕的平面"
  code=""
  showBackBtn="{{showBackBtn}}"
>
  <xr-template-lookat
    disable-scroll
    id="main-frame"
    width="{{renderWidth}}"
    height="{{renderHeight}}"
    style="width:{{width}}px;height:{{height}}px;top:{{top}}px;left:{{left}}px;display:block;"
  />
</xr-demo-viewer>